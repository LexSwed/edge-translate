{
  "root": true,
  "extends": ["eslint:recommended"],
  "plugins": ["svelte3"],
  "parserOptions": {
    "ecmaVersion": 2019,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "webextensions": true
  },
  "overrides": [
    {
      "files": ["**/*.svelte"],
      "processor": "svelte3/svelte3"
    }
  ],
  "globals": {
    "chrome": true
  }
}
