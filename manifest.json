{
  "name": "Edge Translate",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Extension for Microsoft Translator",
  "icons": {
    "16": "icons/nasapod16x16.png",
    "32": "icons/nasapod32x32.png",
    "48": "icons/nasapod48x48.png",
    "128": "icons/nasapod128x128.png"
  },
  "browser_action": {
    "default_popup": "build/popup.html"
  },
  "permissions": ["http://localhost:3000/", "https://*.lexswed.now.sh/"]
}
