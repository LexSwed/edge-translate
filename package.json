{
  "name": "edge-translate",
  "version": "1.0.0",
  "description": "Translator extension for Microsoft Edge using Microsoft Translator",
  "main": "index.js",
  "scripts": {
    "start": "now dev",
    "build": "rollup -c",
    "dev": "run-p rollup serve",
    "rollup": "rollup -c -w",
    "serve": "sirv build --dev --port $PORT",
    "lint": "eslint src/**/*"
  },
  "dependencies": {
    "@now/node": "^1.5.1",
    "autosize": "^4.0.2",
    "axios": "^0.19.2",
    "lodash.debounce": "^4.0.8",
    "node-fetch": "^2.6.0",
    "qs": "^6.9.3"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node-fetch": "^2.5.7",
    "@types/qs": "^6.9.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-svelte3": "^2.7.3",
    "husky": "^4.2.5",
    "now": "^18.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^0.7.0",
    "pretty-quick": "^2.0.1",
    "rollup": "^2.7.2",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "sirv-cli": "^0.4.5",
    "svelte": "^3.21.0",
    "uuidv4": "^6.0.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LexSwed/edge-translate.git"
  },
  "keywords": [
    "edge",
    "extension",
    "chromium"
  ],
